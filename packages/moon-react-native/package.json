{
  "name": "@moonup/moon-react-native",
  "version": "0.0.4",
  "license": "MIT",
  "author": "0xEwhal <ewhal@bytemasons.com>",
  "type": "commonjs",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "prepare": "tsup",
    "size": "size-limit",
    "start": "tsup --watch",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "npm run lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "npm run prettier:fix && npm run lint:fix",
    "build": "tsup src -- --dts-resolve",
    "build:fast": "tsup src",
    "dev": "tsup src --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/moon-react-native.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/moon-react-native.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/properties": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/shims": "^5.7.0",
    "@moonup/ethers": "workspace:*",
    "@moonup/moon-api": "workspace:*",
    "@moonup/moon-sdk": "workspace:*",
    "@react-navigation/native": "^6.1.17",
    "@supabase/supabase-js": "^2.43.5",
    "eip1193-provider": "^1.0.1",
    "ethers": "^5.7.2",
    "expo": "~51.0.8",
    "expo-auth-session": "^5.5.2",
    "expo-router": "^3.5.14",
    "expo-secure-store": "^13.0.1",
    "expo-web-browser": "^13.0.3",
    "react-native-get-random-values": "^1.11.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "^13.10.2"
  },
  "devDependencies": {
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/vite-react": "^3.0.0",
    "@types/bn.js": "^5.1.5",
    "@types/react": "^18.2.66",
    "@types/react-native": "^0.63.35",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^0.34.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.3",
    "react-native-unimodules": "^0.11.0",
    "size-limit": "^11.0.1",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
