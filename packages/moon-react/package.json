{
    "name": "@moonup/moon-react",
    "version": "1.0.22",
    "license": "MIT",
    "author": "0xEwhal <ewhal@bytemasons.com>",
    "type": "commonjs",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "src"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "size-limit": [
        {
            "path": "dist/moon-react.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/moon-react.esm.js",
            "limit": "10 KB"
        }
    ],
    "scripts": {
        "dev-fe": "vite  --port 5360",
        "dev": "vite  --port 5360",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "analyze": "size-limit --why",
        "prepare": "tsup",
        "size": "size-limit",
        "start": "tsup --watch",
        "prettier": "prettier --check src/",
        "prettier:fix": "prettier --write src/",
        "lint:fix": "npm run lint --fix",
        "verify": "run-p prettier lint",
        "verify:fix": "npm run prettier:fix && npm run lint:fix",
        "build": "tsup src -- --dts-resolve",
        "build:fast": "tsup src",
        "tsup-dev": "tsup src --watch",
        "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap --skipLibCheck --outDir dist",
        "test": "vitest",
        "test:watch": "vitest watch",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@moonup/ethers": "workspace:*",
        "@moonup/moon-api": "workspace:^",
        "@moonup/moon-sdk": "workspace:*",
        "@moonup/viem": "workspace:*",
        "@moonup/wagmi-connector": "workspace:*",
        "@rainbow-me/rainbowkit": "^2.1.7",
        "@simplewebauthn/browser": "^10.0.0",
        "@supabase/auth-ui-react": "^0.4.7",
        "@supabase/auth-ui-shared": "^0.1.8",
        "@supabase/supabase-js": "^2.45.4",
        "@tanstack/query-core": "^5.59.0",
        "@tanstack/react-query": "^5.59.0",
        "ethers": "^5.7.2",
        "mipd": "^0.0.7",
        "p-queue": "^8.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.3.0",
        "react-modal": "^3.16.1",
        "react-query": "^3.39.3",
        "react-tabs": "^6.0.2",
        "siwe": "^2.3.2",
        "usehooks-ts": "^3.1.0",
        "uuid": "^8.3.2",
        "viem": "^2.21.19",
        "vite-tsconfig-paths": "^4.3.2",
        "wagmi": "^2.12.17"
    },
    "devDependencies": {
        "@simplewebauthn/typescript-types": "^8.3.4",
        "@tsconfig/vite-react": "^3.0.2",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@types/react-modal": "^3.16.3",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "@vitejs/plugin-react": "^4.3.2",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.12",
        "postcss": "^8.4.47",
        "tailwindcss": "^3.4.13",
        "typescript": "^5.6.2",
        "vite": "^5.4.8"
    }
}
